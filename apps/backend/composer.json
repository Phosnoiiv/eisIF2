{
    "name": "everisay/sif-app-backend",
    "require": {
        "php": "~8.1.0",
        "spiral/framework": "^3.7",
        "spiral/nyholm-bridge": "^1.3",
        "spiral/roadrunner-cli": "^2.5",
        "spiral/roadrunner-bridge": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/php/*"
        }
    ],
    "autoload": {
        "psr-4": {
            "EverISay\\SIF\\Backend\\": "app/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "spiral/composer-publish-plugin": false
        }
    }
}
